@model IEnumerable<Rol>

@{
    ViewData["Title"] = "Roles";
}

<h1>Listado</h1>
<p>
    <a asp-action="CrearRolesBasicos">Crear roles básicos</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
        </tr>
}
    </tbody>
</table>
