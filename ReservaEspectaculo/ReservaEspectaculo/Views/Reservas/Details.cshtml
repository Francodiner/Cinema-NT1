@model ReservaEspectaculo.Models.Reserva

@{
    ViewData["Title"] = "Details";
}

<h1>Ver</h1>

<div>
    <h4>Reserva</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Fecha de la función
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Funcion.Fecha)
        </dd>
        <dt class="col-sm-2">
            Título de la película
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Funcion.Pelicula.Titulo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Activa)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Activa)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cliente)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cliente.Apellido)
            @Html.DisplayFor(model => model.Cliente.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CantidadButacas)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CantidadButacas)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaAlta)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaAlta)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Atras</a>
</div>
