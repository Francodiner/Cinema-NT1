@model ReservaEspectaculo.Models.Reserva

@{
    ViewData["Title"] = "Edit";
}

<h1>Cancelar</h1>

<h4>Reserva</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CancelarPorEmpleado">
            <div asp-validation-summary="ModelOnly" class="text-danger font-weight-bold"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="FechaAlta" class="control-label"></label>
                <input asp-for="FechaAlta" class="form-control" readonly="readonly"/>
                <span asp-validation-for="FechaAlta" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">Apellido del Cliente</label>
                <input asp-for="Cliente.Apellido" class="form-control" readonly="readonly" />
                <span asp-validation-for="Cliente.Apellido" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">Fecha de la función</label>
                <input asp-for="Funcion.Fecha" class="form-control" readonly="readonly" />
                <span asp-validation-for="Funcion.Fecha" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CantidadButacas" class="control-label"></label>
                <input asp-for="CantidadButacas" class="form-control" readonly="readonly"/>
                <span asp-validation-for="CantidadButacas" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Cancelar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Atras</a>
</div>

